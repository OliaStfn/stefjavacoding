 SHOW DATABASES;
 USE max_test_db;
 SHOW TABLES;
 EXPLAIN customers; / DESC customers;
 
 CREATE DATABASE olga_test_db;
 CREATE DATABASE olga_test_db DEFAULT CHARACTER SET utf8 COLLATE utf8_general_ci;

  CREATE TABLE goods(
    -> product_id INT AUTO_INCREMENT PRIMARY KEY,
    -> product_name VARCHAR(30) NOT NULL,
    -> product_price DOUBLE NOT NULL,
    -> product_vendor VARCHAR(30) NOT NULL,
    -> product_production_date DATE NOT NULL,
    -> product_validity_date DATE NOT NULL,
    -> create_date DATE NOT NULL);

    ALTER TABLE goods
    -> DROP product_vendor;

    ALTER TABLE goods
    -> ADD product_vendor VARCHAR(30)
    -> AFTER product_price;

    ALTER TABLE goods
    -> CHANGE product_vendor
    -> product_vendor_name VARCHAR(30);
